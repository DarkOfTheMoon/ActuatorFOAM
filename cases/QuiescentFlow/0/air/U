FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0/air";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 1 -1 0 0 0 0 ];

internalField   uniform ( 0 0 0 );

boundaryField
{
    air_left
    {
        type            advective;
        value           uniform ( 0 0 0 );
    }
    air_up
    {
        type            advective;
        value           uniform ( 0 0 0 );
    }
    air_right
    {
        type            advective;
        value           uniform ( 0 0 0 );
    }
    actuator1_surface
    {
        type            fixedValue;
        value           uniform ( 0 0 0 );
    }
    zfaces
    {
        type            empty;
    }
    air_to_solid
    {
        type            fixedValue;
        value           uniform ( 0 0 0 );
    }
}


