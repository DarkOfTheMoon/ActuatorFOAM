FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue eps 1
    volScalarFieldValue lambda 1
);

regions
(
    zoneToCell
    {
        name solid;

        fieldValues
        (
            volScalarFieldValue eps 2.7
            volScalarFieldValue lambda 1E14
        );
    }
    zoneToCell
    {
        name air;

        fieldValues
        (
            volScalarFieldValue eps 1
            volScalarFieldValue lambda 1.7E-4
        );
    }
);

