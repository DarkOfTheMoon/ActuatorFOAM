// kinematic viscosity of air
nu          [0 2 -1 0 0 0 0]    1.568E-5;

// relative permitivity
eps         [0 0 0 0 0 0 0]     1;

// Debye length
lambda      [0 1 0 0 0 0 0]     1.7E-4;

